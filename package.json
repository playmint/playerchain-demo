{
    "name": "playerchain-demo",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev:watch:gui": "vite --port=3344 --strictPort=true --clearScreen=false --mode=offline",
        "dev:watch:ssc": "ssc build -o --run --host=localhost --port=3344",
        "dev:watch": "run-p --race dev:watch:*",
        "dev": "run-s clean genconfig dev:watch",
        "ios:install": "run-s release:ios && ssc install-app --platform=ios",
        "ios:sim": "run-s clean genconfig start:vite && ssc build --platform=ios-simulator --run",
        "start:vite": "vite build --mode=dev",
        "start:ssc": "ssc build --run",
        "start": "run-s clean genconfig start:*",
        "build:cli": "vite build --mode=production -c cli.vite.ts",
        "build:vite": "vite build",
        "build": "run-s clean build:*",
        "pack:signed": "ssc build --prod -p -c -n",
        "pack:unsigned": "ssc build --prod -p",
        "pack:ios": "ssc build --platform=ios -c -p",
        "genconfig": "ejs socket.template.ini -o socket.ini",
        "release:ios": "run-s genconfig build:vite pack:ios",
        "release:macos": "run-s genconfig build pack:signed",
        "release:win32": "run-s genconfig build pack:unsigned",
        "release:linux": "run-s genconfig build pack:unsigned",
        "test:lint": "eslint .",
        "test:typecheck": "tsc -b",
        "test:build": "vite build --mode=test",
        "Xtest:exec": "node ./scripts/test-harness.mjs",
        "test": "run-s test:*",
        "bootstrap:build": "run-s build:cli",
        "bootstrap:docker": "docker buildx build --progress=plain --platform linux/amd64 -f ./bootstrap/Dockerfile -t playmint/substreamnode:latest --push .",
        "bootstrap": "run-s bootstrap:*",
        "clean:dist": "rimraf dist",
        "clean:build": "rimraf build",
        "clean": "run-s clean:*"
    },
    "dependencies": {
        "@react-three/postprocessing": "^2.16.3",
        "md5": "^2.3.0",
        "postprocessing": "^6.36.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "three.quarks": "^0.15.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.8.0",
        "@fontsource-variable/material-symbols-outlined": "^5.0.37",
        "@fontsource-variable/recursive": "^5.0.18",
        "@fontsource/material-symbols-outlined": "^5.0.37",
        "@rauschma/stringio": "^1.4.0",
        "@react-three/drei": "^9.109.5",
        "@react-three/fiber": "^8.16.8",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/chai": "^4.3.17",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/eslint__js": "^8.42.3",
        "@types/libsodium-wrappers": "^0.7.14",
        "@types/md5": "^2.3.5",
        "@types/mocha": "^10.0.7",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/three": "^0.167.1",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "bitecs": "^0.3.40",
        "cbor-x": "^1.6.0",
        "chai": "^5.1.1",
        "chai-as-promised": "^8.0.0",
        "chalk": "^5.3.0",
        "comlink": "4.4.1",
        "dexie": "^4.0.8",
        "dexie-react-hooks": "^1.1.7",
        "ejs": "^3.1.10",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "fake-indexeddb": "^6.0.0",
        "globals": "^15.9.0",
        "jdenticon": "^3.3.0",
        "libsodium-wrappers": "^0.7.15",
        "lightningcss": "^1.27.0",
        "lru-cache": "^11.0.0",
        "mocha": "^10.7.0",
        "npm-run-all": "^4.1.5",
        "react-error-boundary": "^4.0.13",
        "rimraf": "^6.0.1",
        "three": "^0.167.1",
        "typescript": "5.5.4",
        "typescript-eslint": "^8.0.0",
        "typescript-plugin-css-modules": "^5.1.0",
        "uuid": "^10.0.0",
        "vite": "^5.3.4",
        "vite-plugin-externalize-dependencies": "^1.0.1",
        "vitest": "^2.0.5"
    }
}
