{
    "name": "substream",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "engines": {
        "node": "^20"
    },
    "scripts": {
        "build": "cross-env SSC_ENV=prod ssc build -o --prod -p",
        "build:macos": "cross-env SSC_ENV=prod ssc build -o --prod -p -c -n",
        "dev": "cross-env SSC_ENV=dev ssc build --run",
        "test": "cross-env SSC_ENV=test ssc build --run --test tests/index.js --headless",
        "fix:eslint": "eslint --fix ./src",
        "fix:format": "prettier ./src --write",
        "fix": "run-s fix:*",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "lint-staged": {
        "*.{js,mjs,ts}": [
            "prettier",
            "eslint --fix ./src"
        ]
    },
    "devDependencies": {
        "@eslint/js": "9.6.0",
        "@trivago/prettier-plugin-sort-imports": "4.3.0",
        "@types/eslint-config-prettier": "6.11.3",
        "@types/eslint__js": "8.42.3",
        "@types/three": "0.166.0",
        "cbor-x": "^1.5.9",
        "cross-env": "7.0.3",
        "esbuild": "0.23.0",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "glob": "10.4.2",
        "globals": "15.7.0",
        "husky": "9.0.11",
        "lint-staged": "15.2.7",
        "npm-run-all": "4.1.5",
        "prettier": "3.3.2",
        "typescript": "5.5.3",
        "typescript-eslint": "7.15.0"
    },
    "dependencies": {
        "@dimforge/rapier2d-compat": "^0.13.1",
        "quickjs-emscripten": "^0.29.2",
        "three": "0.166.0"
